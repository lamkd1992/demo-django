{
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        120
    ],
    // Configure editor settings to be overridden for [html] language.
    "[html]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.autoIndent": false
    },
    "[javascript]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true,
        "editor.detectIndentation":false
    },
}